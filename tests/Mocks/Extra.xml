<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE entity SYSTEM
    "https://github.com/nibralab/joomla-architecture/blob/master/code/Joomla/ORM/Definition/entity.dtd">
<entity name="Joomla\ORM\Tests\Mocks\Extra">

    <storage>
        <default table="extras" primary="detail_id"/>
    </storage>

    <fields>
        <field name="info" type="text" default=""/>
    </fields>

    <relations>
        <belongsTo
            name="detail_id"
            entity="Detail"
        />
    </relations>

</entity>
